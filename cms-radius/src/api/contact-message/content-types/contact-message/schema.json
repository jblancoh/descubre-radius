{
  "kind": "collectionType",
  "collectionName": "contact_messages",
  "info": {
    "singularName": "contact-message",
    "pluralName": "contact-messages",
    "displayName": "ContactMessage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "created": {
      "type": "date"
    },
    "message": {
      "type": "text",
      "required": true
    },
    "company": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "customField",
      "customField": "plugin::strapi-phone-validator.phone",
      "options": {
        "country": "mx"
      }
    },
    "originCountry": {
      "type": "enumeration",
      "enum": [
        "mx",
        "usa"
      ],
      "default": "mx",
      "required": true
    }
  }
}
